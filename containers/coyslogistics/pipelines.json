          "auto_actions": ["notify_driver", "send_load_details"]
        },
        {
          "id": "en_route_pickup",
          "name": "En Route to Pickup",
          "order": 3,
          "color": "#8B5CF6",
          "auto_actions": ["start_tracking"]
        },
        {
          "id": "loaded",
          "name": "Loaded",
          "order": 4,
          "color": "#F59E0B",
          "auto_actions": ["upload_bol_reminder"]
