          "order": 1,
          "color": "#6B7280",
          "auto_actions": ["send_welcome_email", "assign_to_sales"]
        },
        {
          "id": "consultation_scheduled",
          "name": "Consultation Scheduled",
          "order": 2,
          "color": "#3B82F6",
          "auto_actions": ["send_calendar_invite"]
