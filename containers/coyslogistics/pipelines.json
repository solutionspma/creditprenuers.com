          "name": "In Transit",
          "order": 5,
          "color": "#EF4444",
          "auto_actions": ["eta_updates"]
        },
        {
          "id": "delivered",
          "name": "Delivered",
          "order": 6,
          "color": "#10B981",
          "auto_actions": ["upload_pod_reminder", "notify_shipper"]
        },
        {
          "id": "invoiced",
          "name": "Invoiced",
          "order": 7,
          "color": "#059669",
          "auto_actions": ["send_invoice"]
