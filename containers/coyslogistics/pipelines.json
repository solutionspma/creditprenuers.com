          "order": 3,
          "color": "#8B5CF6",
          "auto_actions": ["grant_course_access", "send_onboarding_materials"]
        },
        {
          "id": "enrolled_premium",
          "name": "Enrolled - Premium ($497)",
          "order": 4,
          "color": "#EC4899",
