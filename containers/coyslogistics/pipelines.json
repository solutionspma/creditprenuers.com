        {
          "id": "paid",
          "name": "Paid",
          "order": 8,
          "color": "#047857",
          "auto_actions": ["mark_complete"]
        }
      ]
    },
    {
      "id": "driver_onboarding",
      "name": "Driver Onboarding",
      "description": "Track new driver applications and compliance",
      "stages": [
        {
          "id": "application_received",
          "name": "Application Received",
          "order": 1,
          "color": "#6B7280"
