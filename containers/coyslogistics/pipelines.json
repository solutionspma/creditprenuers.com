        {
          "id": "background_check",
          "name": "Background Check",
          "order": 3,
          "color": "#8B5CF6"
        },
        {
          "id": "approved",
          "name": "Approved",
          "order": 4,
          "color": "#10B981",
          "auto_actions": ["grant_app_access", "assign_first_load"]
